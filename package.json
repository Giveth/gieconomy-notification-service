  },
  "homepage": "https://github.com/Giveth/giveconomy-notification-service#readme",
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "async-mutex": "^0.4.0",
    "axios": "^1.2.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.2",
    "patch-package": "^6.5.0",
    "sqlite3": "^5.1.2",
    "typescript": "^4.9.3",
    "web3": "^1.8.1",
    "web3-providers-ws": "^1.8.1",
    "winston": "^3.8.0"
